- var title = ':: Project name ::'
block config

doctype html
html(lang="es")
	head
		meta(http-equiv='Content-Type', content='text/html; charset=utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1, user-scalable=no')
		title #{title}
	body
		include header
		block main
		include footer
